{  "name": "Kaspr : Get Anyone's Phone Numbers and Emails",  "description": "Get access to your prospects' phone number and email, instantly from LinkedIn.",  "version": "1.1.13",  "icons": {    "16": "assets/images/kasprLogo.png",    "48": "assets/images/kasprLogo.png",    "128": "assets/images/kasprLogo.png"  },  "action": {    "default_icon": {      "16": "assets/images/kasprLogo.png",      "48": "assets/images/kasprLogo.png",      "128": "assets/images/kasprLogo.png"    },    "default_title": "Kaspr Plugin",    "default_popup": "./index.html"  },  "permissions": [    "cookies",    "activeTab"  ],  "host_permissions": [    "<all_urls>"  ],  "manifest_version": 3,  "externally_connectable": {    "matches": [      "http://localhost:3000/*",      "https://app.preprod.kaspr.io/*",      "https://app.kaspr.io/*"    ]  },  "background": {    "service_worker":"backgrounds/index.js",    "type" : "module"  },  "content_scripts": [    {      "matches": [        "https://*.linkedin.com/*",        "https://*.kaspr.io/*"      ],      "css": [        "/static/css/linkedinWidget.css"      ],      "js": [        "/static/js/0.chunk.js",        "/static/js/3.chunk.js",        "/static/js/linkedinWidget.js"      ]    }  ],  "web_accessible_resources":[{    "matches": ["*://*/*"],    "resources":  [      "assets/images/logo_round.png",      "assets/images/logo.svg",      "assets/images/creditSm.svg",      "assets/images/user.png",      "assets/images/brandon.png",      "assets/images/notfound.svg",      "assets/images/about.svg",      "assets/images/headquater.svg",      "assets/images/employees.svg",      "assets/images/phoneIcon.svg",      "assets/images/emailIcon.svg",      "assets/images/profileCard1.svg",      "assets/images/profileCard2.svg",      "assets/images/arrow.svg",      "assets/images/userLg.png",      "assets/images/dashboard.svg",      "assets/images/integration.svg",      "assets/images/organizationIcon.svg",      "assets/images/inviteIcon.svg",      "assets/images/teamIcon.svg",      "assets/images/creditIcon.svg",      "assets/images/helpIcon.svg",      "assets/images/myleadsIcon.svg",      "assets/images/profile.gif",      "assets/images/workflow.gif",      "assets/images/kasprLogo.png",      "assets/images/user.png",      "assets/images/syncImg.png",      "assets/images/tick.svg",      "assets/images/kaspr.gif",      "assets/images/kaspr.png",      "assets/images/confetti.svg",      "assets/images/tick3.svg",      "assets/images/linkWhite.svg",      "assets/images/linkBlue.svg",      "assets/images/hubspot-icon.png",      "assets/images/salesforce-icon.png",      "assets/images/sendinblue-icon.png",      "assets/images/pipedrive-icon.png",      "assets/images/lemlist-icon.png",      "assets/images/ic_check_shadow.png",      "assets/images/starG.svg",      "assets/images/logo_loading.gif",      "assets/images/success_check.png",      "assets/images/error-icon.png",      "assets/images/pointsLoading.svg",      "assets/images/pointsLoading2.svg",      "assets/images/redo-solid.svg",      "assets/images/undo-solid.svg",      "assets/images/user-edit-solid.svg",      "assets/images/trash-alt-regular.svg",      "assets/images/edit-regular.svg",      "assets/images/edit-solid.svg",      "assets/images/thumbs-up.svg",      "assets/images/thumbs-up-black.png",      "assets/images/thumbs-up-green.png",      "assets/images/thumbs-up-purple.png",      "assets/images/thumbs-up-red.png",      "assets/images/close-icon.svg",      "assets/images/thinking.svg",      "/assets/images/loop.svg",      "/assets/images/zapier-icon.png"    ]  }]}